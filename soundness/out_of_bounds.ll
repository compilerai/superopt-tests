; ModuleID = '<stdin>'
source_filename = "out_of_bounds"
target datalayout = "e-m:e-p:32:32-f64:32:64-f80:32-n8:16:32-S128"
target triple = "i386-unknown-linux-gnu"

@arr = common dso_local global [4 x i32] zeroinitializer, align 4

; Function Attrs: noinline nounwind
define dso_local i32 @out_of_bounds() #0 {
entry:
  %0 = load i32, i32* getelementptr inbounds ([4 x i32], [4 x i32]* @arr, i32 0, i32 3), align 4
  ret i32 %0
}

